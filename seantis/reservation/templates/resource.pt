<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"

      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="context/main_template/macros/master"
      
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="seantis.reservation"

      xml:lang="en" lang="en">

<body>

<metal:block metal:fill-slot="javascript_head_slot">
    <tal:block tal:replace="structure view/javascript" />
</metal:block>

<metal:block metal:fill-slot="style_slot">
  <style type="text/css">
    .calendarOccupied {
        background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABoAAAAaCAMAAACelLz8AAAAOVBMVEXY2Nj////d7h7d7h7d7h7d7h7///////////////////////////////////////////////////+4yo5sAAAAQklEQVQoke3LqQ3AMADAQKP8b/cftjjAUgaI6cmEmHKprY+59seRSkAlopJQyagUVCoqDZWOykBlorJQ2aj48+Rafr0hD9knm0tJAAAAAElFTkSuQmCC) !important;
        background-repeat: repeat;
        border: solid #ccc 1px;
    }
  </style>
</metal:block>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main">

        <div tal:replace="structure provider:plone.abovecontenttitle" />
        <h1 class="documentFirstHeading" tal:content="context/title" />
        <div tal:replace="structure provider:plone.belowcontenttitle" />
        <div tal:replace="structure provider:plone.abovecontentbody" />

        <div class="documentContent">
          <p tal:content="context/description" />

          <div tal:attributes="id view/calendar_id"></div>

        </div>

        <div tal:replace="structure provider:plone.belowcontentbody" />

    </tal:main-macro>
</metal:main>

</body>

</html>